{
    "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": false,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5"
    },
    "include": [
      "./src/*"
    ]
  }
  